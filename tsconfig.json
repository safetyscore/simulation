{
	"compilerOptions": {
		"alwaysStrict": true,
		"downlevelIteration": true,
		"jsx": "react",
		"jsxFactory": "h",
		"lib": ["dom", "es5", "es2015", "webworker"],
		"module": "None",
		"noImplicitAny": true,
		"noImplicitThis": true,
		"outDir": "site",
		"strictFunctionTypes": true,
		"strictNullChecks": true,
		"target": "es5",
		"skipLibCheck": true
	}
}